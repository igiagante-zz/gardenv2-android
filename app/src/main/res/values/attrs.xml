<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CountView">
        <attr name="setValue" format="integer" />
        <attr name="setHint" format="string" />
    </declare-styleable>

    <declare-styleable name="CountViewDecimal">
        <attr name="setValueDecimal" format="float" />
        <attr name="setDecimalHint" format="string" />
    </declare-styleable>

    <declare-styleable name="CounterView">
        <attr name="setCount" format="integer" />
    </declare-styleable>
</resources>